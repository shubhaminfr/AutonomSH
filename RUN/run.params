  save_average.user_variable = vis_lam
$end_OUTPUT-CONTROL
 
$HPC-DEBUG
  ncell_group = 40
  debug_level = 1
  debug_processor = master
  auto_reorder_boundary_patch = no
  reordering_method = natural
  use_el2part = yes
  check_el2part_consistency = yes
  partitioner = parmetis
$end_HPC-DEBUG

