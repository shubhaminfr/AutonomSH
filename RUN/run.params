$OUTPUT-CONTROL
  save_solution = yes
  save_solution.iteration = 50000
  save_solution.name = ./SOLUT/Run_1
  save_solution.additional = postproc
  save_temporal = yes
  save_temporal.balance = yes
  save_temporal.iteration = 100
  save_temporal.dirname = ./TEMPORALC/
  save_average = yes
  save_average.iteration = 100000
  save_average.freq_averaging.iteration = 10
  save_average.name = ./AVE/18Run_1
  save_average.mode = packages
  save_average.packages = NS_rms wall turbomachinery like
  ! save_average.packages = wall
  ! save_average.mode = packages
  ! save_average.packages = wall NS_rms high_stats wall_high_stats
  ! save_average.user_variable = volc
