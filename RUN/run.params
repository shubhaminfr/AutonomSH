$RUN-CONTROL
  solver_type = ns
  diffusion_scheme = FE_2delta
  simulation_end_iteration = 1000000
  mixture_name = AIR
  reactive_flow  = no
  combustion_model = no
  real_gas = no
  LES_model = wale
  prandtl_turb =   0.60000000D+00
  schmidt_turb =   0.60000000D+00
  convection_scheme = TTGC
  CFL =   0.70000000D+00
  Fourier =   0.10000000D+00
  compute_chemical_timestep = no
  artificial_viscosity_model = Jameson
  artificial_viscosity_2nd_order =   0.50000000D-01
  artificial_viscosity_4th_order =   0.50000000D-02
  clip_species = no
$end_RUN-CONTROL
 
$INPUT-CONTROL
  mesh_file = ../MESH/opt.mesh.h5
  asciibound_file = ../MESH/opt.asciiBound
  solutbound_file = ../SOLUTBOUND/opt.solutBound.h5
  initial_solution_file = ./currentSOLUT/linked.h5
  species_database = species_database.dat
  mixture_database = mixture_database.dat
$end_INPUT-CONTROL

